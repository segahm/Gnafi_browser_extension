<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:em="http://www.mozilla.org/2004/em-rdf#">
	<Description about="urn:mozilla:install-manifest">
		<em:id>{7f82b9b7-8387-4f04-9d6e-09c7618ae52a}</em:id>
		<em:version>0.3.3</em:version>
		<!-- Front End MetaData -->
		<em:name>Gnafi</em:name>
		<em:description>
			Handle the web like one small directory.
		</em:description>
		<em:creator>Sergey Mirkin</em:creator>
		<em:homepageURL>http://gnafi.com/</em:homepageURL>
		<em:optionsURL>
			chrome://gnafi/content/gnafiOptions.xul
		</em:optionsURL>
		<em:iconURL>chrome://gnafi/skin/icon32.png</em:iconURL>
		<!-- Target Application this extension can install into, 
			with minimum and maximum supported versions. -->
		<!-- Firefox -->
		<em:targetApplication>
			<Description>
				<em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
				<em:minVersion>1.0</em:minVersion>
				<em:maxVersion>1.6</em:maxVersion>
			</Description>
		</em:targetApplication>
		<!-- Mozilla -->
		<em:targetApplication>
			<Description>
				<em:id>{86c18b42-e466-45a9-ae7a-9b95ba6f5640}</em:id>
				<em:minVersion>1.5</em:minVersion>
				<em:maxVersion>1.8</em:maxVersion>
			</Description>
		</em:targetApplication>
		<!-- Packages, Skins and Locales that this extension registers -->
		<em:file>
			<Description about="urn:mozilla:extension:file:gnafi.jar">
				<em:package>content/</em:package>
				<em:skin>skin/classic/</em:skin>
				<em:locale>locale/en-US/</em:locale>
			</Description>
		</em:file>
	</Description>
</RDF>
